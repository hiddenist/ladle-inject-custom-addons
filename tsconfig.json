{
  "compilerOptions": {
    "incremental": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["esnext", "dom"],
    "allowJs": false,
    "jsx": "react-jsx",
    "sourceMap": false,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "ladle-inject-custom-addons": ["./src/index.ts"],
      "ladle-inject-custom-addons/*": ["./src/components/*"],
      "ladle-inject-custom-addons/assets/*": ["./assets/*"]
    },
    "types": ["node"]
  },
  "include": ["**/*", ".ladle/components.tsx", ".ladle/example.stories.tsx"],
  "exclude": ["node_modules", "dist", ""]
}
